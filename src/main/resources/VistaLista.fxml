<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="570.0" prefWidth="860.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gestionvehiculos.controller.ControladorLista">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>

   <Pane prefHeight="550.0" prefWidth="801.0">
      <children>
         <TextArea fx:id="listaReservas" editable="false" layoutX="18.0" layoutY="47.0" prefHeight="431.0" prefWidth="785.0" wrapText="true" />
         <Button fx:id="btnEliminar" layoutX="674.0" layoutY="491.0" mnemonicParsing="false" onAction="#eliminarReserva" prefHeight="25.0" prefWidth="132.0" text="Eliminar reserva" />
         <Label fx:id="tituloLista" layoutX="348.0" layoutY="6.0" prefHeight="16.0" prefWidth="125.0" text="Lista de reservas" />
         <Button fx:id="btnVolver" layoutX="18.0" layoutY="491.0" mnemonicParsing="false" onAction="#volver" prefHeight="25.0" prefWidth="132.0" text="Volver" />
      </children>
   </Pane>
</VBox>
